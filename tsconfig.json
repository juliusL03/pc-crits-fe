{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@/apis/*": ["./src/utils/apis/*"],
			"@/forms/*": ["./src/components/common/forms/*"],
			"@/elements/*": ["./src/components/common/elements/*"],
			"@/sections/*": ["./src/components/common/sections/*"],
			"@/layouts/*": ["./src/components/common/layouts/*"],
			"@/modules/*": ["./src/components/modules/*"],
			"@/models/*": ["./src/utils/models/*"],
			"@/store/*": ["./src/utils/store/*"],
   "@/assets/*": ["./src/assets/*"]
		}
	},
	"include": ["next-env.d.ts", "**/*d.ts", "**/*.ts", "**/*.tsx", "json-server/server.js"],
	"exclude": ["node_modules", "**/*.stories.tsx"]
}
